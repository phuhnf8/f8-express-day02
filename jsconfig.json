{
    "compileOnSave": false,
    "compilerOptions": {
        "baseUrl": "./",
        "paths": {
            "@/*": ["src/*"],
            "@/head": ["src/head"],
            "@middleware/*": ["src/middleware/*"],
            "@components/*": ["src/components/*"],
            "@controllers/*": ["src/controllers/*"],
            "@routes/*": ["src/routes/*"],
            "@utils/*": ["utils/*"]
        }
    },
    "include": ["./src/*", "server.js", "src/middlewares/output", "utils"],
    "buildOptions": {}
}
